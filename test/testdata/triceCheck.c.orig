//! SCOPY is a helper macro for struct serialization.
#define SCOPY( element ) do{ char* n = #element; int size = sizeof( src->element ); memcpy( p, &(src->element), size ); p += size; TRICE_S( Id( 1105), "rd:sizeof(%8s)", n ); TRICE( Id( 2283), " = %d\n", size);}while(0);

    //! DCOPY is a helper macro for struct deserialization.
#define DCOPY( element ) do{ char* n = #element; int size = sizeof( dst->element ); memcpy( &(dst->element), p, size ); p += size; TRICE_S( Id( 1333), "rd:sizeof(%8s)", n ); TRICE( Id( 4271), " = %d\n", size);}while(0);


